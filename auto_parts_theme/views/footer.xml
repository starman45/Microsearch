<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <template id="ace_allure_theme_footer" inherit_id="website.snippet_options">
            <xpath expr="//div/we-select[@data-variable='footer-template']" position="inside">
                <we-button title="Geminate auto_parts Footer"
                    data-customize-website-views="auto_parts_theme.auto_parts_new_footer"
                    data-customize-website-variable="'geminate_footer'"
                    data-img="/website/static/src/img/snippets_options/footer_template_default.svg"/>
            </xpath> 
        </template>
        <template id="auto_parts_new_footer" inherit_id="website.layout" name="Geminate Auto Parts">
            <xpath expr="//div[@id='footer']" position="replace">
               <section class="at_prts_footer">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-3">
                                <div class="detalis_left_side_icon pt-2 pt-lg-5" style="color:#fff;">
                                    <div class="icon_1_auto_parts">
                                        <i class="fa fa-home"></i>
                                    </div>
                                    <p class="tittle_p_auto_part">Headquarter:</p>
                                    <p class="detalis_p_auto_part">309, City Center Arcade, Nr. ST Bus Station, Krishnanagar, Ahmedabad, Gujarat - 382345, India New Yark - 110001, USA</p>
                                </div>
                                <div class="detalis_left_side_icon pt-2 pt-lg-5" style="color:#fff;">
                                    <div class="icon_1_auto_parts">
                                        <i class="fa fa-phone"></i>
                                    </div>
                                    <p class="tittle_p_auto_part">Phone:</p>
                                    <p class="detalis_p_auto_part">
                                        <a href="tel:+91 (800) (047) (3006)" style="color:#fff;">+91 (800) (047) (3006)</a>
                                    </p>
                                </div>
                                <div class="detalis_left_side_icon pt-2 pt-lg-5" style="color:#fff;">
